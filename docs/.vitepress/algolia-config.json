{
  "index_name": "lamar_index",
  "start_urls": [
    "https://jasperfx.github.io/lamar/guide"
  ],
  "selectors": {
    "lvl0": {
      "selector": ".VPLink.link.VPNavBarMenuLink.active",
      "global": true
    },
    "lvl1": ".content h1",
    "lvl2": ".content h2",
    "lvl3": ".content h3",
    "lvl4": ".content h4",
    "lvl5": ".content h5",
    "text": ".content p, .content li",
    "lang": {
      "selector": "/html/@lang",
      "type": "xpath",
      "global": true
    }
  },
  "strip_chars": " .,;:#",
  "sielectors_exclude": ["sup"],
  "custom_settings": {
    "attributesForFaceting": ["lang", "tags"]
  }
}
